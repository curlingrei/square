<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<% if @users.any? %>
    <% @users.each do |user| %>
            <div class="name">
              <%= user.username %>
            </div>
            <p><%= link_to 'View profile', user_path(user) %></p>
    <% end %>
  <%= paginate @users %>
<% end %>
<%= javascript_pack_tag 'users/index' %>